{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://meetup-scheduler/venues.schema.json",
  "$comment": "Copyright: See LICENSE.md. Author: Terry Moore",
  "description": "Schema for cached venue data",
  "type": "object",
  "properties": {
    "venues": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name"],
        "properties": {
          "id": { "type": "string" },
          "name": { "type": "string" },
          "address": { "type": "string" },
          "city": { "type": "string" },
          "state": { "type": "string" },
          "country": { "type": "string" }
        }
      }
    }
  }
}
